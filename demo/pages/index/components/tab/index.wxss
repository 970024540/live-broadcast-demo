.i-tabs {
  position: relative;
  box-sizing: border-box;
  display: flex;
  width: 100%;
  padding-top: 20rpx;
  justify-content: flex-start;
  align-items: center;
  white-space: nowrap;
}
.i-tabs-scroll{
  width: 100%;
  padding-top: 20rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: flex-start;
}
