}

.flex-col {
  display: flex;
  flex-direction: column;
}

.line-through {
  text-decoration: line-through;
}

.flex-1-3 {
  width: 33%;
  flex: 0 0 33%;
}

.flex-1-2 {
  width: 50%;
  flex: 0 0 50%;
}

.header {
  padding: 15px 12px;
  background: #fff;
}

.color-tmp {
  color: #dd2642;
}

.space {
  letter-spacing: 1px;
}

.icon {
  position: absolute;
  right: 0;
}

.navibar {
  display: flex;
  padding: 44rpx 0;
  background: #fff;
}

.desc {
  width: 100%;
  display: flex;
  flex-direction: column;
  position: absolute;
  bottom: 0;
  padding: 4rpx 0 4rpx 2rpx;
  background: linear-gradient(to bottom, rgba(0, 0, 0, 0.2), rgba(0, 0, 0, 0.4));
  color: #fff;
}

.video {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  font-size: 100rpx;
  color: rgba(255, 255, 255, 0.3);
}

.ticket-item {
  display: flex;
  margin: 20rpx;
  background-color: #fff;
}

.left {
  padding: 40rpx 60rpx;
}

.flex-col-tmp {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: flex-start;
}

.grab {
  display: inline-block;
  padding: 2rpx 8rpx;
  border: 2rpx solid #dd2451;
  color: #dd2451;
  font-size: 32rpx;
}
.badge-module{
  position: absolute;
  padding:10rpx 20rpx;
}
.badge-left-top{
  top:40rpx;
  left:0;
}
.badge-right-top{
  top:40rpx;
  right:0;
}
.badge-right-bottom{
  right:0;
  bottom:40rpx;
}
.badge-left-bottom{
  left:0;
  bottom:40rpx;
}
.relative{
  position: relative
}

.tagleft {
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
}
.tagright {
  display: flex;
  flex-direction: row-reverse;
  justify-content: space-between;
  align-items: flex-end;
}
.bar-width {
