page {
  width: 100vw;
  height: 100vh;
}

.goods-scroll-cover {
  width: 240rpx;
  height: 240rpx;
}
.swi-bar{
  width: 36rpx;
  height: 10rpx;
  margin: 0 6rpx;
  border-radius: 6rpx;
  background:rgba(0, 0, 0, 0.1);
}
.swi-bar-active{
  background: rgba(0, 0, 0, 0.5);
}
.swi-bar-linear{
  position: absolute;
  width: 100%;
  height: 10rpx;
  margin-top: -14rpx;
  background:rgba(0, 0, 0, 0.1);
  border-radius: 10rpx;
}
.swi-bar-linear-active{
  height: 10rpx;
  border-radius: 10rpx;
  background: rgba(0, 0, 0, 0.5);
  transition: margin-left 250ms ease-in-out 0s;
}
.swiper-inner-class{
  margin-top: -30rpx;
  position: absolute;
  width: 100%;
  background-color: transparent;
}
.swiper-outer-class{
  margin-top: -40rpx;
  position: absolute;
  width: 100%;
}
.goods-list-item-cover {
  width: 320rpx;
  height: 320rpx;
}

.goods-scroll-view_H {
  height: 320rpx;
}

.recommand-title {
  color: rgb(193, 147, 81);
}
.floatBox{
  position:fixed;
  bottom:150rpx;
  right: 0;
  width:20%;
  z-index: 100;
}
.actived-ticket-icon {
  position: absolute;
  top: 20rpx;
  left: 0rpx;
  font-size: 160rpx;
}

.x-countdown {
  color: #e4393c;
  line-height: 1;
}

.padding-r-5 {
  padding-right: 5rpx;
}

.padding-l-5 {
  padding-left: 5rpx;
}
.slide3 {
  overflow: hidden;
  max-height: 0;
  transition: max-height 0.5s cubic-bezier(0, 1, 0, 1) .2s;
}

.animate3 {
  transition-timing-function: cubic-bezier(0.5, 0, 1, 0);
  transition-delay: 0s;
  max-height: 9999rpx;
}
.icon-play {
  position: absolute;
  top: 35%;
  left: 40%;
  font-size: 120rpx;
  color: rgba(0, 0, 0, 0.4);
}

.x-modal {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.2);
  z-index: 99;
}

.modal-close-btn {
  position: absolute;
  top: 0;
  right: 10rpx;
  width: 70rpx;
  height: 70rpx;
  border-radius: 50%;
  background: #e4393c;
  color: #fff;
}

.official-account-box {
  width: 100rpx;
  height: 100rpx;
  overflow: hidden;
  position: fixed;
  bottom: 100rpx;
  right: 10rpx;
  border-radius: 50%;
  overflow: hidden;
  z-index: 999;
  background-color: rgba(50, 161, 74, 0.8);
}

.official-account {
  width: 100rpx;
  height: 100rpx;
  opacity: 0;
  position: absolute;
  top: 0;
  left: 0;
}

