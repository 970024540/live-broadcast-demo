  .ser-home {
    width: 100%;
    height: 100%;
  }
  .red-packet {
    display: block;
    position: relative;
    overflow: hidden;
    width: 100%;
    height: 100%;
  }
  .red-packet li {
    position: absolute;
    animation: all 3s linear;
    top: -700rpx;
    z-index: 10;
  }
  .red-packet a {
    display: block;
  }
  @keyframes aim-move {
    from {
      top: -700rpx;
    }
    to {
      top: 3000rpx;
    }
  }
